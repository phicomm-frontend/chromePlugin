{
  "manifest_version": 2,
  "name": "Phicomm App Navigation",
  "description": "",
  "version": "1.0",
  "icons": {
    "128": "images/nav128.png",
    "16": "images/nav16.png",
    "48": "images/nav48.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/nav128.png",
      "38": "images/nav128.png"
    },
    "default_title": "phicomm"
  },
  "background": {
    "persistent": true,
    "scripts": [ "background.js" ]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  }
}
